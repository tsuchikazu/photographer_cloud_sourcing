<%- model_class = Profile -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:organization_type) %></th>
      <th><%= model_class.human_attribute_name(:organization) %></th>
      <th><%= model_class.human_attribute_name(:division) %></th>
      <th><%= model_class.human_attribute_name(:birth) %></th>
      <th><%= model_class.human_attribute_name(:zip_code_1) %></th>
      <th><%= model_class.human_attribute_name(:zip_code_2) %></th>
      <th><%= model_class.human_attribute_name(:pref) %></th>
      <th><%= model_class.human_attribute_name(:address_1) %></th>
      <th><%= model_class.human_attribute_name(:address_2) %></th>
      <th><%= model_class.human_attribute_name(:phone) %></th>
      <th><%= model_class.human_attribute_name(:mobile_phone) %></th>
      <th><%= model_class.human_attribute_name(:fax) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= link_to profile.id, profile_path(profile) %></td>
        <td><%= profile.name %></td>
        <td><%= profile.organization_type %></td>
        <td><%= profile.organization %></td>
        <td><%= profile.division %></td>
        <td><%= profile.birth %></td>
        <td><%= profile.zip_code_1 %></td>
        <td><%= profile.zip_code_2 %></td>
        <td><%= profile.pref %></td>
        <td><%= profile.address_1 %></td>
        <td><%= profile.address_2 %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.mobile_phone %></td>
        <td><%= profile.fax %></td>
        <td><%= profile.user_id %></td>
        <td><%=l profile.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_profile_path(profile), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      profile_path(profile),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_profile_path,
            :class => 'btn btn-primary' %>
